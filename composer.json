{
    "name": "zend-blog-3/backend",
    "license": "MIT",
    "type": "project",
    "description": "zend-blog ver.3, backend",
    "autoload": {
        "psr-4": { "": "src/" },
        "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
    },
    "require": {
        "ext-xml": "*",
        "ext-pdo_mysql": "*",
        "ext-openssl": "*",
        "symfony/symfony": "3.2.*",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "symfony/swiftmailer-bundle": "^2.3.10",
        "symfony/monolog-bundle": "^3.0.2",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "^2.0",
        "doctrine/doctrine-migrations-bundle": "^1.0",
        "symfony/assetic-bundle": "~2.3",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "symfony/polyfill-apcu": "^1.0",
        "twig/twig": "^1.0||^2.0",
        "league/fractal": "0.12.0",
        "league/flysystem-dropbox": "1.0.*",
        "league/oauth2-client": "~2.2",
        "knplabs/knp-paginator-bundle": "~2.4",
        "friendsofsymfony/jsrouting-bundle": "~1.5",
        "zendframework/zend-filter": "2.7.*",
        "disqus/disqus-php": "dev-master",
        "longman/telegram-bot": "^0.48",
        "kunalvarma05/dropbox-php-sdk": "~0.2",
        "dropbox/dropbox-sdk": "1.1.*"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0",
        "doctrine/doctrine-fixtures-bundle": "2.3.*",
        "morontt/doctrine-to-ember": "dev-master",
        "friendsofphp/php-cs-fixer": "1.13.1",
        "behat/symfony2-extension": "~2.0@dev",
        "behat/mink-extension": "~2.2",
        "behat/mink-goutte-driver": "1.2.*",
        "vipsoft/doctrine-data-fixtures-extension": "dev-master",
        "phpspec/phpspec": "~4.0"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "disqus/disqus-php",
                "version": "dev-master",
                "source": {
                    "type": "git",
                    "url": "https://github.com/disqus/disqus-php.git",
                    "reference": "b1149196ec8aaa9d4dac35a4a28c5647c17ef8af"
                },
                "autoload": {
                    "classmap": [ "disqusapi/" ]
                }
            }
        }
    ],
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-bin-dir": "app",
        "symfony-var-dir": "var",
        "symfony-assets-install": "hard",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "3.2-dev"
        }
    }
}
